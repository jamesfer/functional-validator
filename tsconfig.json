{
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "target": "es5",
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "lib": [
      "es2017"
    ]
  },
  "include": [
    "src/**/*.ts",
    "typings/**/*.ts"
  ]
}
